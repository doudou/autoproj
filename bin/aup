#! /usr/bin/env ruby

ORIGINAL_ARGV = ARGV.dup # This is used in case autoproj needs to update itself

require 'autoproj/cli/update'
Autoproj::Ops::Tools.basic_setup
ops = Autoproj::CLI::Update.new
Autoproj::CmdLine.report do
    args, options = ops.parse_aup_options(ARGV)
    ops.run(args, options)
end
